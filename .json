{
  "version": 2,
  "builds": [
    { "src": "index.php", "use": "@vercel/php" },
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/index.php", "dest": "/index.php" },
    { "src": "/index.html", "dest": "/index.html" }
  ],
  "env": {
    "VARIAVEL_DE_AMBIENTE_1": "VALOR_1",
    "VARIAVEL_DE_AMBIENTE_2": "VALOR_2"
  }
}
